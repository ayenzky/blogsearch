doctype html
html

	head
		title= records.site.sitedata[0].sitename
		meta(charset='utf8')
		meta(http-equiv='X-UA-Compatible', content='IE=edge, chrome=1')
		meta(name='description', content='description of your site')
		meta(name='author', content= "author of the site")
		//- uncomment if your site is responsive!
		meta(name="viewport" content="width=device-width, initial-scale=1")

		meta(name='author', content= records.site.sitedata[0].metauthor)
		meta(name="keywords" content= records.site.sitedata[0].metakeyword)
		meta(name="viewport" content="width=device-width, initial-scale=1")


		link(rel="icon", href= records.site.sitedata[0].icon, type="image/png")
		link(rel="shortcut icon", href= records.site.sitedata[0].icon, type="img/x-icon")


		<script src="//ajax.googleapis.com/ajax/libs/jquery/2.1.4/jquery.min.js"></script>

		<link href="//maxcdn.bootstrapcdn.com/font-awesome/4.6.1/css/font-awesome.min.css" rel="stylesheet">
		<link href="../css/owl.carousel.min.css" rel="stylesheet">
		<link href="../css/custom.css" rel="stylesheet">
		<link href="../css/master.css" rel="stylesheet">



	body.single
		.container
			include includes/header_top

			section#main-content
				.row
					.col.s12.m8.l8.left
						block content
					.col.s12.m4.l4.right
						include includes/sidebar_right


		include includes/footer

		!= js()

		<script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.6/js/materialize.min.js"></script>
		<script src="https://cdnjs.cloudflare.com/ajax/libs/modernizr/2.8.3/modernizr.js"></script>
		script(src="//cdnjs.cloudflare.com/ajax/libs/moment.js/2.11.1/moment.min.js")
		<script src="../js/owl.carousel.min.js"></script>
		<script src="../js/tipuesearch_set.js"></script>
		<script src="../js/tipuesearch.min.js"></script>
		<script src="../js/FeedEk.js"></script>
		<script src="../js/simplecalendar.js"></script>
		<script src="../js/data.js"></script>
		script(src="../js/custom.js")

		<!-- Google Analytics -->
		script(type="text/javascript")
				!= records.site.sitedata[0].analytics



